{
    "exclude": ["**/*.test.ts"],
    "compilerOptions": {
        "target": "ESNEXT",
        "module": "commonjs",
        "outDir": "./dist",
        "downlevelIteration": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
